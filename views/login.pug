extends layout

block content
    h2 #{title}
    form(method='POST' action="/users/login")
        #form-group
            label UserName:
            input.form-control(name='username',type='text')
        #form-group
            label Password:
            input.form-control(name='password',type='password')        
        br
        input.btn.btn-primary(type='submit',value='submit')